<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>List Items Todo</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <td>Id</td>
          <td>Name</td>
          <td>Quantity</td>
          <td>Category Name</td>
          <td>Action</td>
        </tr>
      <thead>
      <tbody>
        <% for (let i=0; i<items.length; i++) { %>
        <tr>
          <td><%= items[i].id %></td>
          <td><%= items[i].name %></td>
          <td><%= items[i].quantity %></td>
          <td><%= items[i].CategoryId %></td>
          <td><%= items[i].actions %></td>
        </td>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
